#include <iostream>
using namespace std;

int main()
{
    int year;
    cout << "Please enter year:";
    cin >> year;
    if (year % 400 == 0 || (year % 100!= 0 && year % 4 == 0)) {
        cout << year << " year is leep year.";
    }
    else {
        cout << year << " is not leap year."<<endl;
    }
}
